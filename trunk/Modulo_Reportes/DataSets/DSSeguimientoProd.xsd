                      empleado ON detallehojaderuta.idlegajo = empleado.idlegajo INNER JOIN
                      torneado ON detallehojaderuta.idtorneado = torneado.idtorneado INNER JOIN
                      tornero ON torneado.idtornero = tornero.idtornero</CommandText>
